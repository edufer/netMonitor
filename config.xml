			<name>Listerner1</name>
			<port>14070</port>
			<command>cmd /c start /wait c:/temp/echo.bat</command>
		</listener>
		<listener>
			<name>Listerner2</name>
			<port>14071</port>
			<command>cmd /c start /wait c:/temp/echo.bat</command>
		</listener>
